{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Országok szerinti fizetési szintek "
  },
  "description": "A simple bar chart with embedded data.",
  "data": {
    "url": "https://raw.githubusercontent.com/krisztakiraly01/krisztakiraly01.github.io/main/salaries.csv",
    "format":{"type":"csv"}
  },
  "mark": "point",
  "encoding": {
    "y": {"field": "salary_in_usd", "type": "quantitative"},
    "x": {"field": "company_location", "type": "nominal"},
    "color": {"field": "remote_ratio", "scale": {"scheme": "darkblue"}}
  },

  "width": 700,
  "height": 300
}